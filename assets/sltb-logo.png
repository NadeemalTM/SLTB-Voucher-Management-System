<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle -->
  <circle cx="100" cy="100" r="95" fill="#2c5530" stroke="#1a3319" stroke-width="3"/>
  
  <!-- Inner circle for contrast -->
  <circle cx="100" cy="100" r="80" fill="#4a7c59" opacity="0.8"/>
  
  <!-- Tea leaf design -->
  <g transform="translate(100,100)">
    <!-- Main leaf shape -->
    <ellipse cx="0" cy="-15" rx="25" ry="45" fill="#7fb069" opacity="0.9"/>
    <ellipse cx="-20" cy="10" rx="20" ry="35" fill="#6fa84f" opacity="0.8"/>
    <ellipse cx="20" cy="10" rx="20" ry="35" fill="#6fa84f" opacity="0.8"/>
    
    <!-- Leaf veins -->
    <line x1="0" y1="-50" x2="0" y2="30" stroke="#2c5530" stroke-width="2"/>
    <line x1="-15" y1="-20" x2="15" y2="-20" stroke="#2c5530" stroke-width="1"/>
    <line x1="-10" y1="0" x2="10" y2="0" stroke="#2c5530" stroke-width="1"/>
  </g>
  
  <!-- Text elements -->
  <text x="100" y="170" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white">
    SLTB
  </text>
  <text x="100" y="185" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="white" opacity="0.9">
    Sri Lanka Tea Board
  </text>
</svg>